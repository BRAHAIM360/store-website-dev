  style.
    body {
      margin: 0;
      padding: 0;
    }
    table {
      border-spacing: 0 !important;
    }
    .wh-100{
      height: 100vh;
      width: 100%;
    }
    .loading-logo{
      text-align: center;
    }
    tbody > tr > td > div.indicator{
      margin-top: 3rem;
      height: 4px;
      width: 200px;
      display: inline-block;
      position: relative;
      background-color: #ddd;
    }
    tbody > tr > td > div.indicator:before{
      position: absolute;
      content: "";
      width: 50%;
      height: 100%;
      background-color: #e00a7f;
      animation: loading-logo 1s infinite;
    }
    .d-none{
      display: none;
    }
    @keyframes loading-logo {
      from {left: 0;}
      50% {left: 50%;}
      to {left: 0;}
    }
    tbody > tr > td > div.logo{
      display: inline-block;
      width: 200px;
      height: 200px;
      background-size: 100%;
      background-position: center;
      background-repeat:no-repeat;
      background-image: url("data:image/svg+xml;base64,");
    }

body
  table#l_loading.wh-100
    tbody.loading-logo
      tr
        td
          div.logo
          br
          div.indicator
  table(id="app-container" class="wh-100" style="display:none;")
    tbody.loading-logo
      tr
        td
          div.logo
          br
          div.indicator
  script.
    function l_show(){
      document.body.removeChild(document.getElementById('l_loading'));
      document.getElementById('app-container').style.display = 'table';
    }
